let mongoose = require('mongoose')

let eventSchema = new mongoose.Schema({
    address: String,
    city: String,
    state: String,
    user: {
        type: mongoose.Schema.Types.ObjectId,
        ref: 'User'
    },
    myKids: {
        type: String,
        default: null
    },
    userInvites: {
        type: String,
        
    },
    date: String,
    time: String
    description: String,
    maxNumberOfGuests: Number

})